{
	"name": "SublimeGoogleSkin",
	"author": "Knose1",
	"version": "1.1.2",
	"description": "Une extension pour le site Google",
	"manifest_version": 2,
	"permissions": [
		"activeTab",
		"https://www.google.com/search*",
		"https://www.google.fr/search*"
	],
	"web_accessible_resources": [
		"script/css/*"
	],
	"content_scripts":[
		{
			"matches": ["https://www.google.com/search*","https://www.google.fr/search*"],
			"js": ["script/index.js"]
		}
	],
	"icons": {
		"16" : "icons/icon16.png",
		"48" : "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
