{
	"name": "SublimeGoogleSkin",
	"author": "Knose1",
	"version": "1.1.2",
	"description": "Une extension pour le site Google",
	"manifest_version": 2,
	"permissions": [
		"activeTab",
		"https://www.google.*/*"
	],
	"web_accessible_resources": [
		"script/css/*"
	],
	"content_scripts":[
		{
			"matches":["https://www.google.*/*"],
			"js": ["styleUtils.js"]
		},
		{
			"matches": ["https://www.google.*/search*"],
			"js": ["styleSearch.js"]
		}
	],
	"icons": {
		"16" : "icons/icon16.png",
		"48" : "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
