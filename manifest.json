{
    "name": "Trap-Gate",
    "author": "Knose1",
    "version": "0.1.0",
    "description": "Une extension pour le site fran√ßais Anime-Gate qui le transforme en Trap-Gate",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "https://www.anime-gate.net/*"
    ],
    "icons": {
        "16": "icons/TrapGateImg.png",
        "48": "icons/TrapGateImg.png",
        "128": "icons/TrapGateImg.png"
    },
    "browser_action": {
        "default_icon": "icons/TrapGateImg.png",
        "default_popup": "redirectAG.html",
        "default_title":"Aller sur Ag"
    },
    "content_scripts": [
        {
            "matches": ["https://www.anime-gate.net/*"],
            "js": ["script/index.js"]
        }
    ]
}
